.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.wrapper {
  width: 90%;
  margin: 0 auto;
  position: relative;
  background-color: blueviolet;
}

.wrapper canvas, .wrapper image {
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
}

.choose-wrapper {
  position: fixed;
  bottom: 10%;
  display: flex;
  flex-direction: column;
}

.choose-wrapper .icon-wrapper {
  display: flex;
  margin-top: 10px;
}

.choose-wrapper .icon{
  width: 80px;
  align-items: center;
  justify-content: center;
  display: flex;
  border: 1px solid #3d3d3d;
  border-radius: 10px;
  margin-right: 10px;
}

.choose-wrapper div image {
  display: flex;
  width: 80px;
}

.choose-wrapper div image.shit {
  width: 60px;
}

#bg {
  opacity: 0;
}

.canvas {
  background-color: #f4f2e6;
  background-size: cover;
  z-index: 100;
}
